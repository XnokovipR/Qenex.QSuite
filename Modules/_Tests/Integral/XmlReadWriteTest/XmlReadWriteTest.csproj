<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RunPostBuildEvent>Always</RunPostBuildEvent>
        <AssemblyName>Qenex.QSuite.XmlReadWriteTest</AssemblyName>
        <RootNamespace>Qenex.QSuite.XmlReadWriteTest</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Qenex.QLibs.XmlInOut" Version="1.0.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\..\..\ModuleXmlHandler\ModuleXmlHandler.csproj" />
    </ItemGroup>

    <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
      <Exec Command="copy &quot;$(ProjectDir)..\..\..\ModuleXmlHandler\Docs\ModuleTest.xml&quot; &quot;$(OutDir)&quot;" />
    </Target>

</Project>
